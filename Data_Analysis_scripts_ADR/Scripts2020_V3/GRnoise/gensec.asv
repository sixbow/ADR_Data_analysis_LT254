function gensec(tabs,title)
%gensec makes the comments lines for seperation section in your code.
tabstr= [''];
for i=1:tabs
    tabstr = [tabstr '\t'];
end
dashstr= [''];
for i=1:tabs
    dashstr = [dashstr '-'];
end
sprintf('\t\t%s+++++|Begin:%s--------------------------------\n','%',title)

end

